<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.SVGPath?>

<StackPane fx:id="backPane" minHeight="1080.0" minWidth="1920.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.client.ui.gui.JFXControllers.LeaderShowGUIController">
   <children>
      <Region fx:id="backRegion" prefHeight="200.0" prefWidth="200.0" />
      <VBox spacing="15.0">
         <children>
                  <Button mnemonicParsing="false" onAction="#openMenu" prefHeight="90.0" prefWidth="150.0" styleClass="darkButton" text="Menu" />
            <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#goBack" prefHeight="90.0" prefWidth="150.0" styleClass="darkButton">
               <graphic>
                  <SVGPath content="M0,20,20,0,20,10,45,10,45,30,20,30,20,40" fill="WHITE" />
               </graphic>
            </Button>
         </children>
         <StackPane.margin>
            <Insets left="30.0" top="30.0" />
         </StackPane.margin>
      </VBox>
      <HBox fx:id="leaderBox" alignment="CENTER" maxWidth="1400.0" prefHeight="100.0" prefWidth="200.0" spacing="20.0" StackPane.alignment="CENTER" />
   </children>
</StackPane>
